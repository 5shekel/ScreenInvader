{
  "makeSmb": [ "storage -c makeSmb", 
    [ "/storage/smb/user", 
      "/storage/smb/password", 
      "/storage/smb/domain", 
      "/storage/smb/host",
      "/storage/smb/directory" ] ],
  "makeNetworkMan": [ "network -c makeNetworkMan",  
    [ "/network/interface",
      "/network/address", 
      "/network/netmask", 
      "/network/gateway" ] ],
  "makeWifi": [ "network -c makeWifi", 
    [ "/network/wifi/ssid", 
      "/network/wifi/encryption/value",
      "/network/wifi/passphrase" ] ],
  "makeNetworkDhcp": [ "network -c makeNetworkDhcp", [ "/network/interface" ] ],
  "makeDns": [ "network -c makeDns", [ "/network/nameserver" ] ],
  "makeHostname": [ "network -c makeHostname", [ "/network/hostname" ] ],
  "reloadNetwork": [ "network -c reloadNetwork", [ "/network/interface" ] ],
  "getWirelessInterfaces": [ "network -c getWirelessInterfaces", [ ] ],
  "getWiredInterfaces": [ "network -c getWiredInterfaces", [ ] ],
  "reloadNetwork": [ "network -c reload", [] ]

  "makeDefaultInittab": [ "inittab", []  ],

  "setVolume": [ "sound -c setVolume", [ "/sound/volume" ] ],
  "getVolume": [ "sound -c getVolume", [] ],
  "incVolume": [ "screen -c incVolume", [] ],
  "decVolume": [ "sound -c decVolume", [] ],
  "setMute": [ "sound -c setMute", [ "/sound/mute" ] ],
  "isMute": [ "sound -c isMute", [] ],
  "toggleMute": [ "sound -c toggleMute", [] ],

  "setBlank": [ "screen -c setBlank", [] ],
  "isBlank": [ "screen -c isBlank", [] ],
  "toggleBlank": [ "sound -c toggleBlank", [] ],
}
